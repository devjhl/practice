<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.practice.spring.dao.MemberDAO">

	<!-- 회원가입 -->
	<insert id="insertMember">
	insert into member(me_id,me_pw,me_email,me_birthday,me_join_time)
		values(#{m.me_id},#{m.me_pw},#{m.me_email},#{m.me_birthday},now())
	</insert>



</mapper>